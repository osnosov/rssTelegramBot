{
  "name": "rss-bot-telegramm",
  "version": "1.1.1",
  "description": "A node.js RSS bot telegramm",
  "main": "app",
  "scripts": {
    "start": "node app.js",
    "migrate": "knex migrate:latest",
    "unmigrate": "knex migrate:rollback",
    "seed": "knex seed:run",
    "lint": "eslint --ext .js, --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js, --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "RSS",
    "Bot",
    "Telegramm"
  ],
  "author": "Oleg Nosov <nosovoleg@gmail.com> ",
  "repository": {
    "type": "git",
    "url": "http://github.com/nosovoleg/rssTelegramBot.git"
  },
  "license": "MIT",
  "dependencies": {
    "cron": "^1.8.2",
    "dotenv": "^8.2.0",
    "feedparser": "^2.2.10",
    "knex": "^2.4.0",
    "mysql": "^2.18.1",
    "node-telegram-bot-api": "^0.50.0",
    "request": "^2.88.2",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "prettier": "^1.19.1"
  }
}
